.head {
    width: 750rpx;
    height: 375rpx;
    margin-top: 20rpx;
}

.headimg {
    height: 375rpx;
    width: 750rpx;
}

.body {
    margin-top: 46rpx;
    padding: 0rpx 20rpx;
    font-size: 30rpx;
    line-height: 56rpx;
    color: #666;
    text-indent: 60rpx;
}

.footer {
    margin-top: 48rpx;
    margin-bottom: 150rpx;
}

.icon {
    height: 28rpx;
    width: 28rpx;
    margin-right: 30rpx;
}

.list {
    display: flex;
    align-items: cneter;
    margin-left: 162rpx;
    color: #666;
    margin-bottom: 22rpx;
}

.fbtn {
    height: 128rpx;
    width: 750rpx;
    background: #fff;
    display: flex;
    justify-content: center;
    align-items: center;
    position: fixed;
    bottom: 0rpx;
    left: 0rpx;
    border-top: 1rpx solid #d5d5d5;
}

.fbtn>button {
    width: 710rpx;
    color: #fff;
    font-size: 34rpx;
    background: #41c2fc;
    height: 98rpx;
    line-height: 98rpx;
}

.gobackindex {
    position: fixed;
    bottom: 100rpx;
    right: 30rpx;
    z-index: 1000;
    background: #ffffff;
    border-radius: 50px;
}

.gobackindex image {
    width: 50px;
    height: 50px;
    border-radius: 50%;
}