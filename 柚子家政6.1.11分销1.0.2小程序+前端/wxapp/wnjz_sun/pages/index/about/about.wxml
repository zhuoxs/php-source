<view class="container">
    <view>
        <view class="head">
            <image class="headimg" src="{{url}}{{aboutsdd.pic}}"></image>
        </view>
        <view class="body">
            <rich-text nodes="{{aboutsdd.details}}"></rich-text>
        </view>
        <view class="footer">
            <view bindtap="dialog" class="list">
                <image class="icon" src="../../../../style/images/icon20.png"></image>
                <text>{{aboutsdd.tel}}</text>
            </view>
            <view class="list">
                <image class="icon" src="../../../../style/images/icon21.png"></image>
                <text>{{aboutsdd.mail}}</text>
            </view>
            <view class="list">
                <image class="icon" src="../../../../style/images/icon22.png"></image>
                <text>{{aboutsdd.address}}</text>
            </view>
        </view>
        <view bindtap="dialogYZ" class="fbtn" style="{{isIpx?'padding-bottom:68rpx':''}}">
            <button>与我联系</button>
        </view>
    </view>
    <view class="gobackindex">
        <navigator openType="redirect" url="/wnjz_sun/pages/index/index">
            <image src="/style/images/indexSelect.png"></image>
        </navigator>
    </view>
</view>
