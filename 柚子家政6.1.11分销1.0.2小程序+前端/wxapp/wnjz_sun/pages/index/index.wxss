.header {
    position: relative;
}

swiper {
    height: 374rpx;
}

.slide-image {
    width: 750rpx;
    height: 374rpx;
}

.search {
    display: flex;
    align-items: center;
    position: absolute;
    bottom: 40rpx;
    left: 75rpx;
    padding-left: 80rpx;
    width: 520rpx;
    height: 70rpx;
    background: rgba(255,255,255,0.88);
    border-radius: 35rpx;
}

.search input {
    width: 400rpx;
}

.searchIcon {
    height: 30rpx;
    width: 30rpx;
    margin-right: 24rpx;
}

.notice {
    width: 100%;
    height: 72rpx;
    background-color: #ffffff;
    line-height: 72rpx;
    font-size: 30rpx;
    font-weight: 700;
    color: #222;
    display: flex;
    align-items: center;
}

.notice image {
    display: flex;
    width: 32rpx;
    height: 32rpx;
    margin: 0rpx 16rpx 0rpx 20rpx;
}

.noticeItem {
    height: 72rpx;
    margin-left: 30rpx;
    font-size: 28rpx;
    color: #666;
    line-height: 72rpx;
    font-weight: 500;
    width: 560rpx;
    overflow: hidden;
}

.userOpe {
    margin-top: 10rpx;
    background-color: #fff;
    padding: 24rpx 20rpx 30rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-around;
}

.opeartion {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.opeartion image {
    width: 82rpx;
    height: 82rpx;
}

.opeartion text {
    font-size: 24rpx;
    color: #222;
    margin-top: 20rpx;
}

.session {
    margin-top: 10rpx;
    padding: 40rpx 20rpx 24rpx;
    background-color: #fff;
}

.session-title {
    font-size: 32rpx;
    font-weight: 700;
    line-height: 32rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
}

.s-title {
    display: flex;
    align-items: center;
}

.s-title:before {
    content: " ";
    display: block;
    width: 10rpx;
    height: 30rpx;
    background-color: rgb(65,194,252);
    border-radius: 16rpx;
    margin-right: 14rpx;
}

.more {
    display: flex;
    font-size: 26rpx;
    font-weight: 500;
    color: #999;
    align-items: center;
}

.more text {
    margin-right: 12rpx;
}

.session-cont {
    display: flex;
    justify-content: flex-start;
    flex-wrap: wrap;
}

.session-list {
    width: 345rpx;
    height: 280rpx;
    overflow: hidden;
    position: relative;
    border-radius: 10rpx;
    margin-top: 24rpx;
    margin-bottom: 6rpx;
}

.session-list:nth-child(odd) {
    margin-right: 20rpx;
}

.service {
    width: 345rpx;
    height: 280rpx;
}

.session-list text {
    display: block;
    background-color: rgba(0,0,0,0.5);
    color: #fff;
    height: 64rpx;
    width: 100%;
    line-height: 64rpx;
    text-align: center;
    position: absolute;
    bottom: 0rpx;
}

.session-order {
    padding: 40rpx 20rpx 24rpx;
}

.session-wrap {
    display: flex;
    flex-direction: column;
    margin-top: 24rpx;
}

.session-item {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding-top: 30rpx;
    padding-bottom: 26rpx;
    border-bottom: 1px solid #ececec;
}

.item-left {
    display: flex;
    color: #999;
}

.order {
    width: 120rpx;
    height: 120rpx;
    margin-right: 20rpx;
}

.item-title {
    font-size: 32rpx;
    line-height: 32rpx;
    margin-top: 6rpx;
    margin-bottom: 16rpx;
    color: #222;
}

.item-desc {
    width: 400rpx;
    overflow: hidden;
    line-height: 32rpx;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.item-price {
    margin-top: 18rpx;
    font-size: 26rpx;
    font-weight: 700;
    color: #41c2fc;
    display: flex;
    align-items: center;
}

.item-price>view {
    font-weight: 500;
    color: #222;
    font-size: 22rpx;
    margin-left: 30rpx;
}

.item-price>view>text {
    color: #41c2fc;
}

.item-price>view>text:first-child {
    margin-right: 20rpx;
}

.item-right {
    height: 64rpx;
    width: 124rpx;
    border: 2rpx solid #41c2fc;
    border-radius: 4rpx;
    font-size: 26rpx;
    font-weight: 700;
    color: #41c2fc;
    line-height: 64rpx;
    text-align: center;
}

.isMask {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 1000;
    background: #000;
    opacity: 0.5;
}

#isLogin {
    position: fixed;
    width: 480rpx;
    height: 320rpx;
    background: #fff;
    top: 50%;
    left: 50%;
    margin-left: -265rpx;
    margin-top: -185rpx;
    z-index: 1001;
    padding: 50rpx 50rpx;
}

.isLoginBtn {
    display: flex;
    justify-content: space-around;
}

.isLoginBtn button {
    background: #fff;
    width: 100%;
    height: 68repx;
    padding-left: 0rpx;
    padding-right: 0rpx;
    margin-right: 0rpx;
    margin-left: 0rpx;
    font-size: 28rpx;
    margin-top: 50rpx;
    color: #4bad37;
}

.loginTitle {
    font-size: 34rpx;
}

.shopImg {
    height: 80rpx;
    display: flex;
    align-items: center;
    font-size: 26rpx;
    padding: 20rpx 0rpx;
    border-bottom: 1rpx solid #ececec;
}

.shopImg>image {
    width: 68rpx;
    height: 68rpx;
    border-radius: 50%;
    margin-right: 20rpx;
}

.isAgary {
    display: flex;
    align-items: center;
    font-size: 24rpx;
    color: #666;
    margin-top: 30rpx;
}

.isAgary>text {
    margin-right: 20rpx;
}

.technical {
    width: 750rpx;
    display: flex;
    padding: 50rpx 0rpx;
    align-items: center;
    justify-content: center;
    font-size: 24rpx;
    color: rgb(153,153,153);
}

.supportIcon {
    width: 64rpx;
    height: 64rpx;
    margin-right: 24rpx;
}

.supportPhone {
    width: 34rpx;
    height: 32rpx;
    margin-bottom: 8rpx;
}

.tecSup {
    line-height: 42rpx;
}

.dialogPhone {
    display: flex;
    flex-direction: column;
    align-items: center;
    margin-left: 60rpx;
    font-size: 20rpx;
}

.detCont {
    width: 414rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.adMask {
    position: fixed;
    top: 0rpx;
    bottom: 0rpx;
    left: 0rpx;
    right: 0rpx;
    background: rgba(0,0,0,0.5);
    z-index: 1000;
}

.ads {
    z-index: 1001;
    position: fixed;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
}

.adIcon {
    width: 54rpx;
    height: 54rpx;
    position: absolute;
    right: 0rpx;
    top: -80rpx;
}

.adImg {
    max-width: 710rpx;
    min-width: 340rpx;
    width: 650rpx;
    max-height: 750rpx;
}

.curshop {
    display: flex;
    padding: 24rpx 20rpx;
    background: #fff;
    margin-top: 10rpx;
}

.curshop view:first-child {
    font-size: 30rpx;
    font-weight: 700;
    line-height: 46rpx;
    width: 570rpx;
}

.toMap {
    width: 136rpx;
    height: 44rpx;
    border-radius: 10rpx;
    border: 2rpx solid #41c2fc;
    color: #41c2fc;
    display: flex;
    align-items: center;
    justify-content: center;
}

.addrIcon {
    width: 28rpx;
    height: 28rpx;
    margin-right: 12rpx;
}

.toIndex {
    background: rgba(255,255,255,0.9);
    z-index: 111;
    width: 96rpx;
    height: 96rpx;
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    box-shadow: 0rpx 0rpx 22rpx 0rpx rgba(184,38,38,0.1);
    position: fixed;
    right: 20rpx;
    bottom: 300rpx;
}

.toIndex>image {
    width: 52rpx;
    height: 50rpx;
}

.ipxFoot {
    height: 200rpx;
}

.preview {
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,0.7);
    position: fixed;
    z-index: 1001;
    top: 0rpx;
}

.preview image {
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    z-index: 999;
    width: 100%;
}

.preview button {
    width: 40%;
    position: absolute;
    bottom: 100rpx;
    left: 30%;
    z-index: 9999;
}

.canvas {
    position: absolute;
    top: -3000rpx;
}