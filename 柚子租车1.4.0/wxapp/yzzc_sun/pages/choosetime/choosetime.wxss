}

.scrollView {
    width: 100%;
    height: 90rpx;
    background-color: #fff;
    white-space: nowrap;
    border-top: 1rpx solid #eee;
    border-bottom: 1rpx solid #eee;
}

.scrollBox {
    min-width: 100%;
    display: inline-block;
    padding-left: 30rpx;
    padding-right: 30rpx;
}

.type-navbar {
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    align-items: center;
    height: 90rpx;
}

.type-box {
    line-height: 84rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    position: relative;
    box-sizing: border-box;
}

.type-navbar-item {
    margin: 0 auto;
    padding: 0 15rpx;
    text-align: center;
    font-size: 30rpx;
}

.type-navbar-item.type-item-on {
    border-bottom: 4rpx solid #ffb62b;
}

.statusTab {
    margin-left: 30rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}

.active {
    position: relative;
}

.active:before {
    content: '';
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    width: 100%;
    height: 4rpx;
    background: #ffb62b;
}

.showOutDis {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 4;
}

.mask {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 5;
    background-color: rgba(0,0,0,0.4);
}

.tishiMOdal {
    position: fixed;
    top: 360rpx;
    left: 10%;
    width: 600rpx;
    height: 342rpx;
    border-radius: 10rpx;
    background-color: #fff;
    display: flex;
    flex-direction: column;
    align-items: center;
    z-index: 6;
    font-size: 30rpx;
}

.icon-wrap {
    margin: 30rpx auto 20rpx;
}

.tishi-icon {
    width: 90rpx;
    display: flex;
}

.tishi-word {
    color: #999;
    text-align: center;
    margin: 0rpx 20rpx 30rpx;
}

.tishi-btn {
    height: 90rpx;
    width: 100%;
    text-align: center;
    line-height: 90rpx;
    color: #ffb62b;
    border-top: 1rpx solid #eee;
    box-sizing: border-box;
}