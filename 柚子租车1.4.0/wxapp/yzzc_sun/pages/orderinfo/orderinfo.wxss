.container {
    width: 100%;
    display: flex;
    flex-direction: column;
}

.about-mes {
    width: 100%;
    background-color: #fff;
    border-top: 1rpx solid #eee;
}

.title {
    display: flex;
    justify-content: space-between;
}

.number {
    font-size: 26rpx;
    color: #999;
    margin: 25rpx 30rpx;
}

.status {
    font-size: 30rpx;
    color: #ffb62b;
    margin: 20rpx 30rpx 20rpx 0rpx;
}

.status.hadPay {
    color: #000;
}

.car-guige {
    display: flex;
    box-sizing: border-box;
    border-top: 1rpx solid #eee;
    border-bottom: 1rpx solid #eee;
}

.carImg {
    width: 200rpx;
    height: 140rpx;
    display: flex;
    margin: 30rpx;
}

.guige-text {
    margin: 65rpx 30rpx 65rpx 0rpx;
}

.carName {
    font-size: 30rpx;
    color: #000;
}

.carMes {
    color: #999;
    font-size: 26rpx;
    margin-top: 10rpx;
}

.box {
    display: flex;
    flex-direction: column;
    padding: 30rpx;
    border-bottom: 1rpx solid #eee;
}

.take-send:nth-child(2) {
    margin-top: 30rpx;
}

.oneRow {
    display: flex;
    align-items: center;
}

.weizhi {
    font-size: 26rpx;
    color: #999;
    margin-left: 30rpx;
}

.storeName {
    color: #000;
    font-size: 30rpx;
    margin-top: 5rpx;
}

.icons {
    width: 30rpx;
    height: 30rpx;
}

.date-box {
    display: flex;
    justify-content: space-between;
    padding-left: 60rpx;
    padding-right: 60rpx;
    border-bottom: 1rpx solid #eee;
}

.dates {
    color: #000;
    font-size: 30rpx;
}

.times {
    color: #999;
    font-size: 26rpx;
    margin-top: 15rpx;
}

.startDate {
    margin: 25rpx 30rpx 30rpx 0rpx;
}

.durations {
    width: 90rpx;
    height: 90rpx;
    border-radius: 100%;
    margin: 20rpx 0rpx;
    border: 1rpx solid #999;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.total {
    color: #ffb62b;
    font-size: 30rpx;
}

.danwei {
    color: #000;
    font-size: 26rpx;
}

.endDate {
    display: flex;
    flex-direction: column;
    align-items: flex-end;
    margin: 25rpx 0rpx 30rpx 30rpx;
}

.zhengjian {
    font-size: 30rpx;
    color: #000;
    padding: 30rpx;
    display: flex;
    align-items: center;
}

.wenhao {
    width: 25rpx;
    height: 25rpx;
    display: flex;
    margin-left: 30rpx;
}

.marginRow {
    width: 100%;
    height: 20rpx;
    background-color: #f2f2f2;
}

.zujin {
    font-size: 30rpx;
    color: #000;
    display: flex;
    justify-content: space-between;
    padding: 30rpx;
}

.zujinMoney {
    color: #ffb62b;
}

.noticeWord {
    width: 92%;
    color: #999;
    font-size: 26rpx;
    margin-top: 20rpx;
}

.payNow-btn {
    margin: 20rpx auto 10rpx;
    width: 92%;
    height: 90rpx;
    border-radius: 10rpx;
    background-color: #ffb62b;
    color: #fff;
    font-size: 34rpx;
}

.cancle-btn {
    margin: 20rpx;
    width: 92%;
    height: 90rpx;
    border-radius: 10rpx;
    background-color: #f2f2f2;
    color: #ffb62b;
    font-size: 34rpx;
    border: 1rpx solid #ffb62b;
}

button::after {
    border: none;
}

.showPopupBox {
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0rpx;
    left: 0rpx;
    z-index: 4;
}

.popupMask {
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0rpx;
    left: 0rpx;
    z-index: 5;
    background-color: rgba(0,0,0,0.4);
}

.lowContents {
    width: 560rpx;
    height: 700rpx;
    border-radius: 10rpx;
    background-color: #fff;
    z-index: 6;
    position: fixed;
    top: 200rpx;
    left: 12%;
    font-size: 36rpx;
    color: #000;
    padding: 15rpx;
    overflow: scroll;
}

.rulesContent {
    width: 100%;
    height: 100%;
    background-color: #fff;
    z-index: 6;
}

.allContents {
    padding: 20rpx;
    display: flex;
    word-break: break-all;
    flex-wrap: wrap;
}