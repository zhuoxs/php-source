<table {{attributes|raw}}>
    <thead>
    <tr>
        {% for header in headers %}
            <th>{{header}}</th>
        {% endfor %}
    </tr>
    </thead>
    <tbody>
    {% for row in rows %}
    <tr>
        {% for item in row %}
        <td>{{item}}</td>
        {% endfor %}
    </tr>
    {% endfor %}
    </tbody>
</table>