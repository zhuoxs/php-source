page {
    background: #f2f2f2;
}

.myOrder,.myOrderItem,.getGoods {
    margin: 0 20rpx;
    background: #fff;
    border-radius: 15rpx;
}

.integral view:nth-child(1) {
    height: 48rpx;
    line-height: 48rpx;
    color: #e9472c;
    font-size: 32rpx;
}

.integral view:nth-child(1),.walletView image {
    margin-top: 20rpx;
}

.myOrder {
    position: relative;
    margin-top: -40rpx;
    padding-bottom: 24rpx;
}

.myOrderCont {
    display: flex;
    align-items: center;
    padding-top: 30rpx;
    justify-content: space-around;
    text-align: center;
}

.myOrderCont image {
    width: 44rpx;
    height: 44rpx;
}

.myOrderTit {
    display: flex;
    align-items: center;
    height: 100rpx;
    padding-left: 30rpx;
    border-bottom: 1rpx solid #eee;
}

.myOrderTit navigator:nth-child(2) {
    flex: 1;
    text-align: right;
}

.myOrderTit image:nth-child(3) {
    margin: 0 30rpx 0 15rpx;
    width: 8rpx;
    height: 14rpx;
}

.mocList1 {
    position: relative;
}

.mocList1>view:nth-child(1) {
    position: absolute;
    height: 24rpx;
    line-height: 24rpx;
    padding: 0 15rpx;
    border-radius: 15rpx;
    background: #e9472c;
    color: #fff;
    font-size: 16rpx;
    margin: -17rpx 0 0 40rpx;
}

.myOrderList {
    height: 100rpx;
    margin: 0 30rpx;
    display: flex;
    align-items: center;
    border-bottom: 1rpx solid #eee;
    font-size: 28rpx;
}

.myOrderList view:nth-child(1) {
    flex: 1;
}

.myOrderList image {
    width: 8rpx;
    height: 14rpx;
}

.myOrderList view:nth-child(2) {
    margin-right: 15rpx;
    color: #999;
    font-size: 24rpx;
}

.wallet {
    position: relative;
    margin-top: -40rpx;
}

.indexBanner {
    width: 100%;
    height: 248rpx;
}

.userPhone {
    margin-top: 8rpx;
    display: flex;
    color: #fff;
    font-size: 24rpx;
}

.userPhone view:nth-child(2) {
    margin-left: 10rpx;
}

.ggTit {
    padding-left: 30rpx;
    height: 100rpx;
    line-height: 100rpx;
    border-bottom: 1rpx solid #eee;
}

.qrcode {
    display: block;
    width: 300rpx;
    height: 300rpx;
    margin: 0 auto;
    margin-top: 60rpx;
}

.getGoods view:nth-child(3) {
    margin-top: 60rpx;
    font-size: 28rpx;
    color: #e9472c;
    text-align: center;
}

.getGoods {
    padding-bottom: 50rpx;
}

.myFinance {
    margin: 20rpx 20rpx 0 20rpx;
    background: #fff;
    border-radius: 15rpx;
}

.myfCont {
    height: 136rpx;
    padding: 0 30rpx;
}

.myfContLef {
    flex: 1;
    justify-content: space-around;
}

.myflItem {
    text-align: center;
}

.myflItem>view:nth-child(1) {
    margin-bottom: 10rpx;
    font-size: 30rpx;
    color: #e9472c;
}

.withdrawal {
    margin-left: 30rpx;
    width: 90rpx;
    height: 50rpx;
    text-align: center;
    line-height: 50rpx;
    color: #fff;
    background: #e9472c;
    border-radius: 8rpx;
}

.header {
    padding-top: 30rpx;
    height: 218rpx;
    display: flex;
    background: #e9472c;
    color: #fff;
}

.headerLef {
    margin: 0 30rpx;
    width: 150rpx;
    height: 150rpx;
    border-radius: 50%;
}

.name,.address {
    font-size: 24rpx;
}

.trange {
    margin: 10rpx 0;
    font-size: 30rpx;
}

.scan {
    width: 46rpx;
    height: 46rpx;
    position: absolute;
    right: 30rpx;
    top: 40rpx;
}

.name {
    margin-top: 15rpx;
}

.mfBot {
    padding-left: 30rpx;
    height: 88rpx;
    display: flex;
    align-items: center;
    border-top: 1rpx solid #eee;
}

.mfbLef {
    flex: 1;
    display: flex;
}

.mfbLef view {
    color: #e9472c;
}

.myfContRig {
    margin-right: 30rpx;
}

.orderCount {
    position: absolute;
    top: -10rpx;
    left: calc(50% + 5rpx);
    min-width: 36rpx;
    height: 36rpx;
    line-height: 36rpx;
    border-radius: 18rpx;
    background-color: red;
    color: white;
}

.goodsCont {
    height: 136rpx;
    display: flex;
    align-items: center;
}

.addGoods {
    margin-right: 20rpx;
    width: 130rpx;
    height: 62rpx;
    line-height: 62rpx;
    background: #ff7800;
    border-radius: 10rpx;
    color: #fff;
    font-size: 28rpx;
    text-align: center;
}

.verificationMember,.nonmyFinance {
    display: flex;
    align-items: center;
    height: 100rpx;
    padding-left: 30rpx;
    margin: 20rpx 20rpx 0 20rpx;
    background: #fff;
    border-radius: 15rpx;
}

.nonmyFinance {
    display: flex;
    align-items: center;
    padding: 15rpx 30rpx;
    margin: 20rpx 20rpx 0 20rpx;
    background: #fff;
    border-radius: 15rpx;
}

.vmRig {
    margin-right: 20rpx;
    height: 62rpx;
    padding: 0 15rpx;
    line-height: 62rpx;
    background: #ff7800;
    border-radius: 10rpx;
    color: #fff;
    font-size: 28rpx;
}

.addGoodslef,.addGoodsrig {
    flex: 1;
}

.goodsLine {
    width: 0;
    height: 100rpx;
    border-left: 1rpx solid #eee;
}

.addGoodsrig {
    display: flex;
    justify-content: flex-end;
}

.addGoodslef {
    text-align: center;
}

.addGoodslef .v1 {
    margin-bottom: 10rpx;
    font-size: 30rpx;
    color: #e9472c;
}

.vmLef {
    flex: 1;
}

.vmLef text {
    color: #e9472c;
}

.headTable {
    margin: 20rpx 20rpx 0 20rpx;
    display: flex;
    flex-wrap: wrap;
    background: #fff;
    border-radius: 15rpx;
}

.htList {
    width: 33.333%;
    box-sizing: border-box;
    border-right: 1rpx solid #eee;
    border-bottom: 1rpx solid #eee;
}

.htTit {
    margin: 20rpx 0 0 30rpx;
    height: 36rpx;
    line-height: 36rpx;
    font-size: 28rpx;
}

.htNum {
    padding: 20rpx 0 30rpx 0;
    text-align: center;
    font-size: 32rpx;
    color: #e9472c;
}

.htList:nth-child(3n) {
    border-right: 0;
}

.customer-header {
    background-color: var(--cusbg);
}

.apply {
    position: relative;
}

.apply image {
    width: 100%;
}

.apply .btn-apply {
    position: absolute;
    bottom: 60rpx;
    left: 23rpx;
    right: 23rpx;
    background-color: #FF0027;
    color: #fff;
    text-align: center;
    padding: 20rpx;
    font-size: 35rpx;
    border-radius: 15rpx;
}

.omask {
    position: fixed;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    background: rgba(0,0,0,0.3);
    z-index: 11;
}

.getGoods {
    position: fixed;
    left: 4%;
    top: 50%;
    width: 92%;
    transform: translateY(-50%);
    background: #fff;
    border-radius: 15rpx;
    padding-bottom: 50rpx;
    z-index: 12;
}

.getGoods view:nth-child(3) {
    margin-top: 60rpx;
    font-size: 28rpx;
    color: #e9472c;
    text-align: center;
}

.orderCount {
    position: absolute;
    top: -10rpx;
    left: calc(50% + 5rpx);
    min-width: 36rpx;
    height: 36rpx;
    line-height: 36rpx;
    border-radius: 18rpx;
    background-color: red;
    color: #fff;
    font-size: 22rpx;
}

.userName text:nth-child(2) {
    margin-left: 9rpx;
    font-size: 24rpx;
}

.wallet,.myOrder,.myOrderItemCysc {
    margin: 0 20rpx;
    margin-top: 20rpx;
    background: #fff;
    border-radius: 15rpx;
}

.myOrderItemCysc image {
    width: 80rpx;
    height: 80rpx;
    border-radius: 50%;
}

.myOrderItemCysc {
    display: flex;
    align-items: center;
    flex-wrap: wrap;
    padding-bottom: 30rpx;
}

.myOrderItemCysc navigator {
    width: 25%;
    text-align: center;
    padding-top: 45rpx;
}