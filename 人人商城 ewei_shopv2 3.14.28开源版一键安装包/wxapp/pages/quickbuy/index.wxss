}

.carList .goodsMsg view:nth-of-type(1) {
    line-height: 36rpx;
    padding-top: 30rpx;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
}

.carList .goodsMsg view:nth-of-type(2) {
    color: #b3b3b3;
    font-size: 24rpx;
    padding-bottom: 30rpx;
}

.money {
    color: #ff5555;
    margin: 48rpx 30rpx 0 0;
}

.carList .addShopCar {
