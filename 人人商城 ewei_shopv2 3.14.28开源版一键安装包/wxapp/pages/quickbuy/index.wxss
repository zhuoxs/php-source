    font-size: 28rpx;
}

.total {
    float: left;
}

.shopCar2 i {
    position: absolute;
    top: -2rpx;
    right: -10rpx;
    width: 40rpx;
    height: 40rpx;
    border-radius: 50%;
    background: #e40100;
    display: block;
    text-align: center;
    font-size: 22rpx;
