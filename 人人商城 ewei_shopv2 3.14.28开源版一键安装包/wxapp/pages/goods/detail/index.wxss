    z-index: 1;
}

.fui-number .minus:before,.fui-number .plus:before {
    content: " ";
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    border-left: 1px solid #d9d9d9;
}

.fui-number .minus:after,.fui-number .plus:after {
    content: " ";
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    border-right: 1px solid #d9d9d9;
}

.fui-number .minus.disabled,.fui-number .plus.disabled {
