page {
}

.container {
}

.avatar_wrap {
    width: 100%;
}

.avatar {
    position: relative;
    top: -150rpx;
    z-index: 9;
    text-align: center;
    border-bottom: 1px solid #eee;
    padding-bottom: 20rpx;
}

.member_wrap {
    margin-bottom: 15rpx;
}

.credit-box {
    margin: 5px 0 0;
    text-align: center;
    color: #aaa;
    font-size: 14px;
}

.like-btn {
    padding: 3px 8px;
    background: #f60;
    color: #fff;
    border-radius: 10px;
    display: inline-block;
    margin-left: 10px;
    font-size: 12px;
}

.nav_wrap {
    margin-top: -150rpx;
}

.my-avatar {
    width: 160rpx;
    height: 160rpx;
    border-radius: 50%;
    margin-bottom: 10rpx;
}

.my-panel {
    background: #fff;
    margin-bottom: 20rpx;
    width: 100%;
    overflow: hidden;
}

.nav_wrap {
    display: flex;
    text-align: center;
    justify-content: space-around;
}

.nav-list {
}

.nav-list navigator {
    padding: 20rpx 40rpx;
}

.list-icon {
    margin: 0 auto;
}

.list-icon image {
    width: 70rpx;
    height: 70rpx;
}

.nav-text {
    font-size: 14px;
    color: #666;
}

.my-icon,.wxapp-icon {
    width: 40rpx;
    height: 40rpx;
    margin-right: 10rpx;
    vertical-align: middle;
}

.wxapp-item {
    padding: 12px 15px;
    border-bottom: 1px solid #eee;
    font-size: 14px;
    position: relative;
}

.wxapp-item::after {
    position: absolute;
    top: 50%;
    right: 17px;
    content: " ";
    display: inline-block;
    height: 6px;
    width: 6px;
    border-width: 2px 2px 0 0;
    border-color: #c8c8c8;
    border-style: solid;
    transform: translateY(-50%) matrix(.71,.71,-.71,.71,0,0);
}

.wxapp-title {
    margin-left: 5px;
    display: inline-block;
    vertical-align: middle;
}

.version {
    text-align: center;
    font-size: 12px;
    color: #ccc;
    margin-bottom: 25px;
}

.freeze {
    margin-left: 5px;
}

.pop-img {
    width: 500rpx;
}

.bt-info {
    font-size: 14px;
    color: #f60;
}

.service {
    width: 100%;
    height: 40px;
    position: absolute;
    left: 0;
    top: 0;
    background-color: transparent;
}

.contact-btn {
    padding: 0;
    background-color: transparent;
    line-height: 80rpx;
}

.contact-btn .iconfont {
    font-size: 24px!important;
}

.contact-btn image {
    width: 60rpx;
    height: 60rpx;
    margin-top: 10rpx;
}