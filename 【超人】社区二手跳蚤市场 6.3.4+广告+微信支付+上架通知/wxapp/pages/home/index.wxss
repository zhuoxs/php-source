.container {
    position: relative;
}

.wel-box {
    position: fixed;
    top: 30%;
    left: 50%;
    margin-left: -300rpx;
    z-index: 99999;
}

.wel-img {
    width: 600rpx;
    display: block;
    position: relative;
}

.wel-btn {
    background-color: transparent!important;
    width: 400rpx;
    height: 500rpx;
    position: absolute!important;
    top: 0;
    left: 50%;
    margin-left: -200rpx!important;
    z-index: 1000000;
}

.wel-btn:after {
    border: 0;
}

.top-search {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    z-index: 99;
    padding: 15rpx;
    background: #fff;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.city-box {
    font-size: 14px;
    color: #666;
    padding-right: 8rpx;
}

.city-box .iconfont {
    margin-left: 6rpx;
}

.search-icon {
    color: #888;
}

.top-search .search-box {
    background: #eee;
    padding: 4rpx 15rpx;
    border-radius: 30rpx;
    box-sizing: border-box;
    flex: 1;
}

.top-search input {
    background: transparent;
    width: 85%;
    font-size: 14px;
    padding-left: 18rpx;
    display: inline-block;
    vertical-align: middle;
}

.top-search #searchBtn {
    display: none;
}

.top-location {
    width: 100%;
    position: absolute;
    left: 0;
    z-index: 10;
    text-align: center;
}

.location-box {
    display: inline-block;
    font-size: 14px;
    max-width: 100%;
}

.location-box .iconfont {
    font-size: 18px;
    margin-right: 2px;
}

.location-list {
    background: #000;
}

.location-item {
    padding: 4px 0 4px 20px;
    text-align: left;
    border-bottom: 1px dashed #ccc;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}

.location-item:last-child {
    padding-bottom: 0;
    border: none;
}

.round-box {
    padding: 0 6px;
    background-color: rgba(0,0,0,.3);
    border-radius: 30rpx;
    color: #fff;
}

.search-box {
    position: absolute;
    z-index: 10;
    width: 40px;
    text-align: center;
}

.search-box .iconfont {
    font-size: 18px;
}

.screen-swiper image {
    pointer-events: auto;
}

.banner {
    height: 240px;
    overflow: hidden;
}

.swiper image {
    width: 100%;
    max-height: 100%;
}

.notice-swiper {
    padding: 20rpx;
    height: 78rpx;
    background: #fff;
    margin: 20rpx 0;
}

.notice-icon {
    width: 40rpx;
    height: 40rpx;
    float: left;
    margin-right: 10rpx;
}

.notice {
    width: 650rpx;
    height: 40rpx;
    line-height: 40rpx;
    font-size: 14px;
    color: #999;
    float: right;
}

.cate-panel {
    padding: 15px 0 0;
}

.cate-zan-col,.album-zan-col {
    text-align: center;
    position: relative;
}

.nav .cu-item {
    font-size: 16px;
    font-weight: bold;
}

.album-zan-col {
    margin-bottom: 15rpx;
}

.item-list {
    border-top: 1px solid #eee;
}

.item-album {
    max-width: 100%;
}

.item-panel {
    padding: 5px 15px;
}

.item-footer {
    padding: 10px 15px 0;
    border-top: 1px solid #eee;
}

.play-mask {
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    width: 94%;
    background: rgba(0,0,0,.4);
    text-align: center;
    line-height: 180rpx;
    z-index: 22;
}

.play-mask .iconfont {
    font-size: 60rpx;
    color: #fff;
}

.itemad-cover {
    width: 50%;
}

.guide-box {
    position: fixed;
    padding-right: 20rpx;
    right: 10px;
    top: 80px;
    z-index: 9999;
}

.arrow {
    float: right;
    margin-right: 110rpx;
    margin-bottom: 15px;
}

.arrow .iconfont {
    font-size: 50px;
    color: #fff;
}

.guide-text {
    float: right;
    color: #fff;
    font-size: 16px;
    line-height: 24px;
}

.item-distance {
    font-size: 12px;
    color: #F60;
    position: absolute;
    top: 82rpx;
    right: 30rpx;
}

.set-top {
    border: 1px solid #f60;
    padding: 0 10rpx;
}

.notice-list {
    background: #fff;
    padding: 0 20rpx;
    margin: 20rpx 0;
}

.notice-item {
    font-size: 14px;
    color: #888;
    padding: 16rpx 0;
    border-bottom: 1px solid #f2f2f2;
}

.notice-item text {
    display: inline-block;
    max-width: 660rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

official-account {
    position: fixed;
    bottom: 110rpx;
    width: 100%;
    z-index: 99;
}

.official-account-close {
    position: fixed;
    right: 4rpx;
    bottom: 230rpx;
    z-index: 100;
    color: #ccc;
}

.like-btn {
    padding: 3px 8px;
    background: #f60;
    color: #fff;
    border-radius: 10px;
    display: inline-block;
    margin-left: 3px;
    font-size: 12px;
}

.diy-col-25 {
    width: 25%;
}

.diy-col-40 {
    width: 40%;
}

.diy-col-50 {
    width: 50%;
}

.diy-col-60 {
    width: 60%;
}

.diy-col-100 {
    width: 100%;
}

.cubeimg-content {
    width: 100%;
    margin: 14rpx 0;
}

.cubeimg-content .diy-col {
    float: left;
    padding: 6rpx;
    box-sizing: border-box;
}

.cube-img {
    width: 100%;
    height: 200rpx;
    display: block;
}

.cubeimg-height1 {
    height: 200rpx;
}

.cubeimg-height2 {
    height: 400rpx;
}

.cubeimg-height3 {
    height: 600rpx;
}

.cubeimg-height4 {
    height: 194rpx;
}

.cubeimg-height5 {
    height: 388rpx;
}

.cubeimg-top-one {
    margin-bottom: 12rpx;
}

.no-padding {
    padding: 0!important;
}

.cubeimg-top {
    margin-bottom: 12rpx;
}

.cubeimg-bottom-one {
    padding: 0 6rpx 0 0!important;
}

.cubeimg-bottom-two {
    padding: 0 0 0 6rpx!important;
}