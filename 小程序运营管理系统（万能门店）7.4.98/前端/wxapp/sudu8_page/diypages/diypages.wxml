<template name="show_page_personlist">
    <view class="fwzxBtn" style="padding:{{style.pdh*2}}rpx {{style.pdw*2}}rpx;background:{{params.styledata==1?'url('+params.backgroundimg+')':''}} {{params.positiony}} {{params.positionx}} {{params.repeat}} {{style.background}};background-size:{{params.size==1?'cover':style.sizew+'%'}} {{params.size==1?'':style.sizeh+'%'}};margin-top:{{style.mt*2}}rpx;box-sizing:border-box;">
        <view bindtap="redirectto" class="hbj" data-link="/sudu8_page/staff_card/staff_card?id={{item.id}}" data-linktype="page" style="padding:30rpx 0;border-bottom:2rpx solid #eee" wx:for="{{data}}">
            <image src="{{item.pic}}" style="width:108rpx;height:108rpx;border-radius:50%"></image>
            <view style="margin-left:20rpx;flex:1">
                <view style="font-size:28rpx;color:#434343;">{{item.realname}}</view>
                <view class="hbj">
                    <view style="font-size:24rpx;color:#838383;">{{item.job}}</view>
                    <view style="height:24rpx;background:#eee;width:2rpx;margin:0 16rpx;"></view>
                    <view style="font-size:24rpx;color:#838383">{{item.age==0?'保密':item.age+'岁'}}</view>
                    <view style="height:24rpx;background:#eee;width:2rpx;margin:0 16rpx;"></view>
                    <view style="font-size:24rpx;color:#838383">{{item.province}}{{item.city}}</view>
                </view>
                <view class="hbj">
                    <image mode="widthFix" src="/sudu8_page/resource/img/personlist_star.png" style="width:28rpx;margin-right:12rpx;" wx:if="{{item.score>0}}"></image>
                    <image mode="widthFix" src="/sudu8_page/resource/img/personlist_star.png" style="width:28rpx;margin-right:12rpx;" wx:if="{{item.score>1}}"></image>
                    <image mode="widthFix" src="/sudu8_page/resource/img/personlist_star.png" style="width:28rpx;margin-right:12rpx;" wx:if="{{item.score>2}}"></image>
                    <image mode="widthFix" src="/sudu8_page/resource/img/personlist_star.png" style="width:28rpx;margin-right:12rpx;" wx:if="{{item.score>3}}"></image>
                    <image mode="widthFix" src="/sudu8_page/resource/img/personlist_star.png" style="width:28rpx;margin-right:12rpx;" wx:if="{{item.score>4}}"></image>
                    <view style="font-size:24rpx;color:#838383">{{item.score}}分</view>
                </view>
            </view>
            <view>
                <view class="person_list_btnbox" style="text-align:right">
                    <view class="person_list_btn" style="display:inline-block;width:100rpx;height:44rpx;line-height:44rpx;text-align:center;background-color:#2F74FD;color:#fff;font-size:28rpx;border-radius:8rpx;" wx:if="{{item.contract==1}}">签约</view>
