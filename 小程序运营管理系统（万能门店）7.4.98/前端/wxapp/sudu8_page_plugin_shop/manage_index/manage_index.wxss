.mTop {
    height: 228rpx;
    background: #6671E4;
    color: #fff;
    padding: 30rpx 30rpx 0;
    box-sizing: border-box;
    line-break: 60rpx;
    display: flex;
}

.mTop image {
    width: 90rpx;
    height: 90rpx;
    margin-right: 20rpx;
    border-radius: 50%;
    border: 2rpx solid #fff;
}

.mTop view {
    padding-top: 26rpx;
    font-size: 30rpx;
}

.mData {
    width: 680rpx;
    margin: -75rpx auto 0;
    background: #fff;
    border-radius: 10rpx;
    padding: 35rpx 0;
    text-align: center;
    box-shadow: 2rpx 2rpx 8rpx #ddd;
}

.mDatat1 {
    font-size: 30rpx;
}

.mDatat2 {
    font-size: 50rpx;
    font-weight: bold;
    color: #6671E4;
    margin-bottom: 30rpx;
}

.mDatat2 span {
    font-size: 24rpx;
    padding-left: 6rpx;
}

.mDataList {
    display: flex;
}

.mDataListLine {
    width: 33.33%;
}

.mDataListLine>view {
    color: #969696;
    margin-bottom: 10rpx;
    font-size: 28rpx;
}

.mDataListLine .mDataListLineNum {
    font-size: 32rpx;
    color: #000;
}

.mMenu {
    padding: 35rpx;
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
}

.mMenuRow {
    width: 48%;
    margin-bottom: 30rpx;
    background: #fff;
    box-shadow: 2rpx 2rpx 8rpx #eee;
    font-size: 30rpx;
    padding: 30rpx 50rpx;
    box-sizing: border-box;
    border-radius: 10rpx;
    display: flex;
}

.mMenuRow image {
    width: 66rpx;
    height: 66rpx;
    border-radius: 4rpx;
}

.mMenuRow view {
    padding: 8rpx 0 0 30rpx;
}