page {
    height: 100%;
    background: #f9f9f9;
}

.button_imgsed {
    width: 80%;
    height: 85rpx;
    background-color: #333;
    color: #fff;
}

.page_modle {
    height: 96%;
    width: 690rpx;
    display: block;
    margin: 0 auto;
    margin-top: 5%;
    background: #FFFFFF;
    box-shadow: 0 9rpx 16rpx 0 rgba(0,0,0,0.10);
    border-radius: 10rpx;
    overflow: hidden;
}

.page_modle_top {
    height: 510rpx;
    width: 100%;
}

.page_modle_topImg {
    height: 510rpx;
    width: 100%;
}

.page_modle_topview {
    position: absolute;
    height: 410rpx;
    border-radius: 10rpx 10rpx 0 0;
    width: 690rpx;
}

.page_modle_topview_headImg {
    height: 90rpx;
    width: 90rpx;
    display: block;
    margin: 0 auto;
    border: 2rpx solid #FFFFFF;
    margin-top: 40rpx;
    border-radius: 100%;
}

.page_modle_toptitle {
    font-size: 30rpx;
    color: #FFFFFF;
    width: 100%;
    margin-top: 15rpx;
    text-align: center;
}

.page_modle_radius {
    width: 600rpx;
    margin: 0 auto;
    display: block;
    padding-top: 20rpx;
    padding-bottom: 20rpx;
    border: 1px solid #FFFFFF;
    margin-top: 40rpx;
    border-radius: 10rpx;
}

.page_modle_radius_top {
    font-size: 35rpx;
    width: 90%;
    margin-left: 5%;
    font-weight: bold;
    text-align: center;
    color: #fff;
}

.page_modle_radius_btm {
    font-size: 25rpx;
    width: 90%;
    margin-left: 5%;
    margin-top: 30rpx;
    color: #fff;
    text-align: center;
}

.phone_view {
    width: 630rpx;
    height: 72rpx;
    background: #F8F8F8;
    display: block;
    margin: 0 auto;
    border: 1rpx solid rgba(0,0,0,0.10);
    border-radius: 10rpx;
    margin-top: 70rpx;
}

.phone_view_img {
    height: 43rpx;
    width: 42rpx;
    padding-right: 15rpx;
    border-right: 1rpx solid rgba(0,0,0,0.10);
    float: left;
    margin-left: 20rpx;
    margin-top: 16.5rpx;
}

.phone_input {
    font-size: 24rpx;
    color: #333;
    height: 72rpx;
    padding-left: 20rpx;
    line-height: 72rpx;
}

.erweima {
    height: 300rpx;
    width: 300rpx;
    margin: 0 auto;
    display: block;
    margin-top: 63rpx;
}

.tishi {
    width: 100%;
    text-align: center;
    margin-top: 50rpx;
    font-size: 26rpx;
    color: #666666;
}

.btn {
    background: #0AC2AE;
    border-radius: 15rpx;
    height: 72rpx;
    width: 630rpx;
    margin: 0 auto;
    margin-top: 40rpx;
    display: block;
    text-align: center;
    font-size: 28rpx;
    color: #FFFFFF;
    line-height: 72rpx;
}

.left_dian {
    height: 35rpx;
    width: 35rpx;
    background: #f9f9f9;
    border-radius: 100%;
    position: absolute;
    top: 530rpx;
    left: 15rpx;
}

.left_dian_sec {
    height: 35rpx;
    width: 35rpx;
    background: #f9f9f9;
    border-radius: 100%;
    position: absolute;
    top: 530rpx;
    right: 15rpx;
}