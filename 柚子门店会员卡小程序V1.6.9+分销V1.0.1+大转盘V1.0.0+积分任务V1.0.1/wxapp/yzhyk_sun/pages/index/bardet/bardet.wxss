}

.progressItem {
    position: absolute;
    height: 28rpx;
    background-color: #f5ac32;
    border-radius: 14rpx;
    top: 0rpx;
    right: 0rpx;
}

.progressTop {
    position: absolute;
    top: -70rpx;
    height: 50rpx;
    padding: 0rpx 16rpx;
    color: #fff;
    background-color: #f5ac32;
    border-radius: 25rpx;
    line-height: 50rpx;
    font-size: 24rpx;
    margin-right: -76rpx;
}

