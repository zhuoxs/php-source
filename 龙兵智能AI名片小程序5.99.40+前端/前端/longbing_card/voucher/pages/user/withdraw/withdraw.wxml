<form bindsubmit="formSubmit">
    <view class="withdraw-account-sec">
        <view class="text fl">到账账户</view>
        <input class="fl" name="account" placeholder="请输入微信账号"></input>
    </view>
    <view class="withdraw-money-sec" style="margin-top:0rpx;">
        <view class="withdraw-title" style="margin-top:20rpx;">提现金额</view>
        <view class="withdraw-money rel">
            <view class="abs">￥</view>
            <input focus="{{true}}" name="money" value="{{inputMoney}}"></input>
        </view>
        <view class="withdraw-content">钱包余额￥{{dataList.profit}}<block wx:if="{{dataList.profit>0}}">, <text bindtap="toJump" data-money="{{dataList.profit}}" data-status="toWithdrawAll">全部提现</text>
            </block>
        </view>
        <view class="withdraw-content" style="font-size:24rpx;padding-top:5rpx;">最低提现额度 <text>￥{{dataList.cash_mini}}</text>
        </view>
        <button class="withdraw-btn br-10 tc" data-status="toWithDrawBtn" formType="submit" hoverClass="none">提现</button>
    </view>
</form>
