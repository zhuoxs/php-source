page{
    background: #f0f0f4;
}
view,button{ 
    display: block;
    overflow: initial;
}  
.setTab-box-view{
    width: 100%;
    padding: 0rpx;
    border: transparent; 
    margin-bottom: 12rpx;
    position: fixed;
} 
.swiper-tab-list{
    color: #323232;
}
.boss-index-sec-2 .spread-count-list-sec .child .content .flex{
    height: 40rpx;
    line-height: 40rpx;
}
.boss-index-sec-2 .spread-count-list-sec .child .content .flex .tr {
    font-size:24rpx;
    color:#686868;
}
.boss-index-sec-2 .spread-count-list-sec .child .content .time{
    font-size:24rpx;
    color:#969696;
}
.spread-count-list-sec .child:nth-child(1) i,
.spread-count-list-sec .child:nth-child(2) i,
.spread-count-list-sec .child:nth-child(3) i{
    background: none;
    font-size: 40rpx;
}
.boss-index-sec-2 .spread-count-list-sec .child .content .flex .flex100-7 em{
    font-size: 18rpx;
    color: white;
    width: 54rpx;
    height: 30rpx;
    line-height: 30rpx;
    display: inline-block;
    background: #367abd;
    margin-left: 10rpx;
    top: -4rpx;
}
.boss-index-sec-2 .spread-count-list-sec .child .content .flex .flex100-7 em.second{
    background: #ffae00;
}