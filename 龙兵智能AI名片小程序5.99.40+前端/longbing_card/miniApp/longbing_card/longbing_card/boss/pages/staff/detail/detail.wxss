 
page{
    background: #f4f4f8;
}
view,button{ 
    display: block;
    overflow: initial;
}
.setTab-box-view{
    width: 100%;
    padding: 0rpx;
}
.swiper-tab-list{
    padding: 0rpx;
}
.setTab-box-view .active::before { 
  content: ''; 
  margin-left: -60rpx;
  width: 120rpx; 
}
.radar-sec .child,
.radar-sec .child .dynamic .dynamic-child,
.radar-sec .child .dynamic .dynamic-child .flex{
    height: auto;
}
.radar-sec .child .dynamic .dynamic-child .flex{
    min-height: 90rpx;
}
.radar-sec .child{
    margin-top: 0rpx;
}
.radar-sec .child .time{
    margin-top: 12rpx;
}
.radar-sec .child .dynamic .dynamic-child{
    padding-bottom: 0rpx;
}
.radar-sec .child .time{
    border-bottom:1rpx solid #ededed;
}
.radar-sec .child .dynamic{
    border: transparent;
}