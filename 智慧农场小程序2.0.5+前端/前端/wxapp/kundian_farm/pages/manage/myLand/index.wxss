@import "../../../../__wuBaseWxss__/2.wxss";

page {
    background-color: #f4f4f4;
}

.container {
    width: 94%;
    margin: 20rpx auto;
    border-radius: 20rpx;
    background-color: #fff;
    box-shadow: 0 2rpx 5rpx rgba(238,238,238,0.8);
    overflow: hidden;
    padding-bottom: 20rpx;
}

.title_name {
    font-size: 40rpx;
    color: #fc8535;
    font-weight: 400;
    letter-spacing: 3rpx;
    position: relative;
    text-align: center;
}

.sup {
    position: absolute;
    right: 15rpx;
    top: 0rpx;
    font-size: 20rpx;
}

.title_desc {
    color: #ffc096;
    font-size: 32rpx;
    text-align: center;
    margin-top: 10rpx;
    letter-spacing: 3rpx;
}

.orderInfo_container {
    width: 100%;
    margin-top: 40rpx;
    border-top: 1rpx dashed #fc8535;
    border-bottom: 1rpx dashed #fc8535;
    padding: 30rpx;
    box-sizing: border-box;
    font-size: 28rpx;
    color: #1e1b1c;
}

.head {
    color: #6a6767;
    width: 200rpx;
}

.upload {
    width: 100%;
    height: 130rpx;
    border-radius: 20rpx;
    background-color: #fc8535;
    margin-top: 30rpx;
    color: #fff;
    font-size: 34rpx;
    text-align: center;
    overflow: hidden;
}

.icon {
    width: 35rpx;
    height: 35rpx;
}

.seed_container {
    width: 90%;
    border-radius: 10rpx;
    border: 1rpx solid #dadddc;
    margin: 30rpx auto;
    padding-bottom: 20rpx;
    font-size: 28rpx;
}

.seed-info {
    width: 95%;
    margin-left: 5%;
    height: 60rpx;
    line-height: 60rpx;
}

.seed-info .info-title {
    width: 35%;
    color: gray;
}

.seed-btn {
    width: 90%;
    margin-left: 5%;
    height: 90rpx;
}

.seed-btn .seed-status {
    width: auto;
    padding: 0 10rpx;
    height: 50rpx;
    line-height: 50rpx;
    border-radius: 50rpx;
    border: 1rpx solid #09BB07;
    color: #09BB07;
    margin-right: 20rpx;
    font-size: 24rpx;
}

.seed_title {
    font-size: 32rpx;
    font-weight: 400;
    color: #454545;
    padding: 20rpx 30rpx;
    box-sizing: border-box;
    letter-spacing: 3rpx;
    border-left: 10rpx solid #09BB07;
    width: 90%;
    margin-left: 5%;
    margin-top: 20rpx;
    background: #f4f4f4;
}

.seed_item {
    display: flex;
    align-items: center;
    padding: 20rpx 30rpx;
    box-sizing: border-box;
}

.seedImg {
    width: 160rpx;
    height: 160rpx;
    border-radius: 15rpx;
    border: 1rpx solid #dadddc;
    overflow: hidden;
}

.seedImg image {
    width: 100%;
    height: 100%;
}

.seedInfo {
    margin-left: 30rpx;
    color: #a09e9e;
    font-size: 34rpx;
}

.seedName {
    width: 350rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    color: #2e2a2b;
}

.total {
    padding: 10rpx 30rpx;
    box-sizing: border-box;
    display: flex;
}

.total_title {
    font-size: 38rpx;
    font-weight: 600;
    color: #2e2a2b;
    height: 50rpx;
    line-height: 50rpx;
}

.payTime {
    font-size: 34rpx;
    color: #676666;
    padding: 10rpx 30rpx;
    box-sizing: border-box;
    display: flex;
    align-items: center;
}

.borderBottom {
    border-bottom: 1rpx solid #dadddc;
}

.block20 {
    height: 20rpx;
}

.output-mask {
    width: 100%;
    height: 100%;
    position: fixed;
    background: black;
    opacity: 0.7;
    top: 0;
    z-index: 99;
}

.output-info {
    width: 94%;
    margin-left: 3%;
    height: 500rpx;
    background: white;
    z-index: 999;
    background: white;
    border-radius: 10rpx;
    position: fixed;
    top: 10%;
    font-size: 28rpx;
    padding: 20rpx 0;
}

.output-title {
    width: 90%;
    margin-left: 5%;
    height: 80rpx;
    line-height: 80rpx;
    border-bottom: 1rpx solid #f4f4f4;
}

.info-ul {
    width: 90%;
    margin-left: 5%;
    height: 90rpx;
    line-height: 90rpx;
    margin-top: 20rpx;
}

.info-ul input {
    border: 1rpx solid #f4f4f4;
    width: 94%;
    height: 70rpx;
    padding-left: 20rpx;
}

.info-ul .info-ul-value {
    margin-left: 20rpx;
    width: 62%;
}

.info-ul .unit {
    height: 70rpx;
    width: 70rpx;
    border: 1rpx solid #f4f4f4;
    line-height: 70rpx;
    text-align: center;
}

.ouput-btn {
    width: 60%;
    margin-left: 20%;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    background: #09BB07;
    border-radius: 80rpx;
    color: white;
    margin-top: 70rpx;
}

.output-title .icon-delete1 {
    float: right;
    font-size: 35rpx;
    color: gray;
}

button::after {
    border: none;
}

.ouput-btn button {
    background: #09BB07;
    color: white;
    height: 80rpx;
    line-height: 82rpx;
    border-radius: 80rpx;
}

.info-value input {
    border: 1rpx solid #f4f4f4;
    padding-left: 10rpx;
    font-size: 26rpx;
    border-radius: 10rpx;
}