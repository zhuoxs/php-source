.water {
    bottom: 0;
    height: 0;
    width: 100%;
    opacity: 1;
    position: absolute;
    z-index: 99;
}

.water_show {
    height: 40%;
    transition: 3s;
    transform: translateZ(0);
}

.water-1,.water-2,.water-3 {
    position: absolute;
    width: 100%;
    bottom: 0;
    left: 0;
}

.water-1 {
    height: 100%;
    opacity: 0.3;
    background-size: 100%;
    animation: wave-animation-1 2.5s infinite linear;
}

.water-2 {
    height: 100%;
    opacity: 0.6;
    background-size: 100%;
    animation: wave-animation-2 4s infinite ease-in-out;
}

.water-3 {
    height: 100%;
    background-size: 100%;
    animation: wave-animation-3 6.5s infinite linear;
}

@-webkit-keyframes wave-animation-1 {
    0% {
        background-position: 0 top;
    }

    100% {
        background-position: 600px top;
    }
}

@keyframes wave-animation-1 {
    0% {
        background-position: 0 top;
    }

    100% {
        background-position: 600px top;
    }
}

@-webkit-keyframes wave-animation-2 {
    10% {
        background-position: 0 top;
    }

    100% {
        background-position: 600px top;
    }
}

@keyframes wave-animation-2 {
    10% {
        background-position: 0 top;
    }

    100% {
        background-position: 600px top;
    }
}

@-webkit-keyframes wave-animation-3 {
    20% {
        background-position: 0 top;
    }

    100% {
        background-position: 600px top;
    }
}

@keyframes wave-animation-3 {
    20% {
        background-position: 0 top;
    }

    100% {
        background-position: 600px top;
    }
}

