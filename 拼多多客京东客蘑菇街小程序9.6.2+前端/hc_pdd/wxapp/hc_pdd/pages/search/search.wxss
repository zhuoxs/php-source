.hengxie::before {
    content: '\200b';
    width: 30%;
    height: 3rpx;
    background: #000;
    position: absolute;
    left: 35%;
    bottom: 0;
}

.jage,.jagetwo,.jagethree {
    position: absolute;
    width: 40rpx;
    height: 84rpx;
    right: 10rpx;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    padding: 30rpx 0;
    box-sizing: border-box;
}

.qiyta {
    position: relative;
    width: 10rpx;
    height: 10rpx;
}

.qiytat {
    position: relative;
    width: 10rpx;
    height: 10rpx;
}

.qiyta::before {
    content: "\200b";
    width: 0;
    height: 0;
    position: absolute;
    top: 0;
    left: 4%;
    border-left: 5rpx solid transparent;
    border-right: 5rpx solid transparent;
    border-bottom: 10rpx solid #a7a7a7;
}

.qiytat::before {
    content: "\200b";
    width: 0;
    height: 0;
    position: absolute;
    top: 0;
    left: 4%;
    border-left: 5rpx solid transparent;
    border-right: 5rpx solid transparent;
    border-top: 10rpx solid #a7a7a7;
}

.jagetwo .qiyta::before {
    content: "\200b";
