.banner>image {
    width: 750rpx;
}

.session {
    background: #faf9f9;
    padding: 30rpx 30rpx 30rpx;
}

.list {
    background: #fff;
    margin-bottom: 30rpx;
    box-shadow: 0rpx 0rpx 24rpx 0rpx rgba(3,3,3,0.086);
    border-radius: 10rpx;
}

.list-up {
    width: 690rpx;
    height: 390rpx;
    position: relative;
}

.list-up image {
    width: 690rpx;
    height: 390rpx;
    border-radius: 10rpx 10rpx 0rpx 0rpx;
}

.activeType {
    position: absolute;
    right: 12rpx;
    top: 12rpx;
    width: 150rpx;
    height: 48rpx;
    background: #f25551;
    color: #fff;
    border-radius: 10rpx;
    text-align: center;
    line-height: 48rpx;
}

.brand {
    display: inline-block;
    padding: 0rpx 30rpx 0rpx;
    margin-top: 20rpx;
    background: #f5ac32;
    color: #fff;
    height: 50rpx;
    line-height: 50rpx;
    overflow: hidden;
}

.lst {
    padding: 0rpx 30rpx 0rpx;
}

.list-title {
    font-weight: 700;
    margin-bottom: 20rpx;
    margin-top: 16rpx;
    line-height: 38rpx;
}

.list-time {
    color: #666;
    font-size: 24rpx;
    display: flex;
    justify-content: space-between;
    padding-bottom: 32rpx;
}

.list-time text {
    margin-right: 10rpx;
}

.list-time .num {
    color: #f5ac32;
}

.btn {
    width: 154rpx;
    height: 62rpx;
    background: #f5ac32;
    border-radius: 10rpx;
    text-align: center;
    line-height: 62rpx;
    color: #fff;
}

.pass,.pass image {
    position: absolute;
    right: 12rpx;
    top: 12rpx;
    width: 113rpx;
    height: 98rpx;
    z-index: 12;
}

.mask {
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,0.5);
    z-index: 9;
    position: absolute;
    top: 0rpx;
    left: 0rpx;
}

.plate_form {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
}

.plate_form button {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    background: none;
}

.plate_form button::after {
    border: none;
}

swiper {
    height: 230rpx;
}

.slide-image {
    width: 750rpx;
    height: 230rpx;
}