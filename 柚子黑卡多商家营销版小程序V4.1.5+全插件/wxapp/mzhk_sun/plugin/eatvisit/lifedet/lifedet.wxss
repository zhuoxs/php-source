.wxParse{ margin: 0 5px; font-family: Helvetica,sans-serif; font-size: 28rpx; color: #666; line-height: 1.8; }
.wxParse-inline{ display: inline; margin: 0; padding: 0; }
.wxParse-div{margin: 0;padding: 0;}
.wxParse-h1{ font-size:2em; margin: .67em 0 }
.wxParse-h2{ font-size:1.5em; margin: .75em 0 }
.wxParse-h3{ font-size:1.17em; margin: .83em 0 }
.wxParse-h4{ margin: 1.12em 0}
.wxParse-h5 { font-size:.83em; margin: 1.5em 0 }
.wxParse-h6{ font-size:.75em; margin: 1.67em 0 }
.wxParse-h1 { font-size: 18px; font-weight: 400; margin-bottom: .9em; }
.wxParse-h2 { font-size: 16px; font-weight: 400; margin-bottom: .34em; }
.wxParse-h3 { font-weight: 400; font-size: 15px; margin-bottom: .34em; }
.wxParse-h4 { font-weight: 400; font-size: 14px; margin-bottom: .24em; }
.wxParse-h5 { font-weight: 400; font-size: 13px; margin-bottom: .14em; }
.wxParse-h6 { font-weight: 400; font-size: 12px; margin-bottom: .04em; }
.wxParse-h1, .wxParse-h2, .wxParse-h3, .wxParse-h4, .wxParse-h5, .wxParse-h6, .wxParse-b, .wxParse-strong { font-weight: bolder }
.wxParse-i,.wxParse-cite,.wxParse-em,.wxParse-var,.wxParse-address{font-style:italic}
.wxParse-pre,.wxParse-tt,.wxParse-code,.wxParse-kbd,.wxParse-samp{font-family:monospace}
.wxParse-pre{white-space:pre}
.wxParse-big{font-size:1.17em}
.wxParse-small,.wxParse-sub,.wxParse-sup{font-size:.83em}
.wxParse-sub{vertical-align:sub}
.wxParse-sup{vertical-align:super}
.wxParse-s,.wxParse-strike,.wxParse-del{text-decoration:line-through}
.wxParse-strong,.wxParse-s{display: inline}
.wxParse-a{ color: deepskyblue; word-break:break-all; overflow:auto; }
.wxParse-video{ text-align: center; margin: 10px 0; }
.wxParse-video-video{ width:100%; }
.wxParse-img{ overflow: hidden; display: block; }
.wxParse-blockquote { margin: 0; padding:10px 0 10px 5px; font-family:Courier, Calibri,"宋体"; background:#f5f5f5; border-left: 3px solid #dbdbdb; }
.wxParse-code,.wxParse-wxxxcode-style{ display: inline; background:#f5f5f5; }
.wxParse-ul{ margin: 20rpx 10rpx; }
.wxParse-li,.wxParse-li-inner{ display: flex; align-items: baseline; margin: 10rpx 0; }
.wxParse-li-text{ align-items: center; line-height: 20px; }
.wxParse-li-circle{ display: inline-flex; width: 5px; height: 5px; background-color: #333; margin-right: 5px; }
.wxParse-li-square{ display: inline-flex; width: 10rpx; height: 10rpx; background-color: #333; margin-right: 5px; }
.wxParse-li-ring{ display: inline-flex; width: 10rpx; height: 10rpx; border: 2rpx solid #333; border-radius: 50%; background-color: #fff; margin-right: 5px; }
.wxParse-u { text-decoration: underline; }
.wxParse-hide{ display: none; }
.WxEmojiView{ align-items: center; font-size:28rpx; }
.wxEmoji{ width: 16px; height:16px; }
.wxParse-tr{ display: flex; border-right:1px solid #e0e0e0; border-bottom:1px solid #e0e0e0; border-top:1px solid #e0e0e0; }
.wxParse-th, .wxParse-td{ flex:1; padding:5px; font-size:28rpx; border-left:1px solid #e0e0e0; word-break: break-all; }
.wxParse-td:last{ border-top:1px solid #e0e0e0; }
.wxParse-th{ background:#f0f0f0; border-top:1px solid #e0e0e0; }
.wxParse-del{ display: inline; }
.wxParse-figure { overflow: hidden; }
.container { background-color: #fff; }
.over{ overflow-y: hidden; }
.cont-over{ overflow-y: hidden; }
.container>view { padding-bottom: 50rpx; }
.wrap { margin: 0rpx 20rpx; width: 710rpx; }
.title { margin-top: 26rpx; margin-bottom: 20rpx; line-height: 48rpx; font-size: 32rpx; font-weight: 700; }
.goods-sm { font-size: 26rpx; color: #666; }
.goods-sm .more-active { display: inline-block; margin: 0 10rpx 0 20rpx; color: #003e9c; }
.shop { display: flex; align-items: center; justify-content: flex-end; margin-top: 20rpx; color: #f5ac32; font-weight: 700; }
.shop>image { height: 24rpx; width: 32rpx; margin-right: 15rpx; animation: move 1s infinite }
@keyframes move{ 0%{ transform: translate(-10rpx,-5px); }
50%{ transform: translate(0rpx); }
100%{ transform: translate(-10rpx,-5px); }
}.cont { display: flex; flex-direction: column; align-items: center; margin-top: 38rpx; padding-bottom: 30rpx; width: 100%; background-image: -webkit-linear-gradient(90deg, rgb(139, 17, 142) 0%, rgb(252, 30, 71) 100%); border-radius: 10rpx; color: #fff; }
.prize-msg { display: flex; align-items: center; width: 510rpx; height: 60rpx; padding-left: 40rpx; margin-top: 30rpx; border-radius: 8rpx; background-color: rgba(255, 255, 255, 0.4); font-size: 32rpx; }
.swiper { margin-left: 10rpx; height: 60rpx; width: 310rpx; line-height: 62rpx; overflow: hidden; }
.user-num { margin-top: 20rpx; font-size: 32rpx; }
.chance-num { margin-top: 48rpx; margin-bottom: 40rpx; }
.chance-num>text { color: #f6ff01; font-weight: 700; font-size: 34rpx; }
.get-chance { margin-top: 10rpx; margin-bottom: 30rpx; width: 396rpx; height: 58rpx; line-height: 58rpx; text-align: center; border-radius: 29rpx; border: 2rpx solid #fff; font-size: 32rpx; font-weight: 700; }
.detail { margin-top: 30rpx; width: 590rpx; padding: 40rpx; border-radius: 10rpx; background-color: rgba(255, 255, 255, 0.4); }
.detail>view { display: flex; align-items: center; }
.name { white-space: nowrap; }
.des { flex: 1; display: flex; justify-content: space-between; align-items: center; line-height: 36rpx; }
.detail>view:nth-child(2) { margin: 30rpx 0 16rpx; }
.detail>view.address { display: flex; }
.icon { flex: 0 0 48rpx; width: 48rpx; height: 48rpx; margin-left: 20rpx; }
.session { margin-top: 50rpx; color: #fff; }
.session-title { position: relative; margin-left: 163rpx; font-size: 34rpx; font-weight: 700; }
.session-title>view { width: 384rpx; height: 66rpx; line-height: 66rpx; background-color: #f7bd5b; border: 4rpx solid #fff; border-radius: 10rpx; text-align: center; letter-spacing: 2rpx; }
.content { padding: 33rpx 20rpx 50rpx; width: 670rpx; border-radius: 8rpx; background-color: rgb(247, 189, 91); margin-top: -33rpx; }
.award { margin-top: 40rpx; line-height: 36rpx; text-align: center; font-weight: 700; }
.level { margin-bottom: 22rpx; font-size: 30rpx; color: #222; }
.banner { margin-top: 30rpx; }
.banner, .banner>image { width: 710rpx; height: 90rpx; }
.rule { margin-top: 30rpx; padding: 30rpx 20rpx 40rpx; width: 666rpx; border: 2rpx solid rgb(245, 172, 50); border-radius: 8rpx; }
.rule-title { line-height: 34rpx; font-size: 32rpx; font-weight: 700; text-align: center; letter-spacing: 6rpx; }
.rule-cont { margin-top: 30rpx; line-height: 42rpx; color: #999; }
.mask { position: fixed; top: 0rpx; bottom: 0; right: 0; left: 0; height: 100%; width: 100%; background-color: rgba(0, 0, 0, 0.4); z-index: 3; overflow: hidden; }
.model { position: fixed; top: 180rpx; left: 90rpx; height: 800rpx; width: 570rpx; z-index: 4; background-color: #fff; border-radius: 10rpx; overflow: hidden; animation: showModel 0.6s; }
.modelBg { position: absolute; height: 800rpx; width: 570rpx; border-radius: 10rpx; }
.model-content { position: absolute; display: flex; flex-direction: column; align-items: center; width: 570rpx; border-radius: 10rpx; }
.model-head { display: flex; align-items: center; justify-content: space-between; margin-top: 50rpx; width: 358rpx; }
.model-head .txt { flex: 1; margin: 0 20rpx 0 20rpx; line-height: 40rpx; font-size: 36rpx; font-weight: 700; text-align: center; }
.model-head .line { flex: 0 0 100rpx; height: 4rpx; width: 100rpx; background-color: rgb(34, 34, 34); }
.model-title { margin-top: 48rpx; line-height: 54rpx; width: 200rpx; font-size: 48rpx; font-weight: 700; color: #ff8b24; text-align: center; }
.prize-cont{ position: relative; width: 490rpx; height: 300rpx; margin-top: 50rpx; }
.prizebg{ position: absolute; width: 490rpx; height: 300rpx; }
.prize-detail{ position: absolute; width: 490rpx; text-align: center; }
.prize-level{ margin-top: 58rpx; line-height: 36rpx; font-size: 34rpx; font-weight: 700; color: #f6ff00; }
.prize-name{ margin-top: 44rpx; line-height: 34rpx; font-size: 30rpx; font-weight: 700; color: #fff; }
.prize-check{ position: absolute; bottom: 30rpx; width: 490rpx; font-size: 26rpx; text-align: center; }
.congratulate{ margin-top: 42rpx; padding:0 54rpx; line-height: 32rpx; text-align: center; color: #fe3737; font-size: 26rpx; }
wx-cover-view{ white-space: normal; }
.model-foot .btn{ display: inline-block; margin:46rpx 20rpx 0; width: 210rpx; height: 62rpx; line-height: 62rpx; background-color: rgb(214, 214, 214); border-radius: 31rpx; color: #666666; text-align: center; font-size: 34rpx; font-weight: 700; }
.model-foot .confirm{ background-color: rgb(255, 130, 62); color: #fff; }
@keyframes showModel{ 0%{ transform: translateY(-100%) }
100%{ transform:translateY(0); }
}.canvas{ width: 295px; height: 295px; position: relative; z-index: 0; }
.canvas>canvas{ z-index: 0; }
.start{ height: 112px; width: 90px; position: absolute; top:50%; left: 50%; margin-left: -45px; margin-top: -66px; z-index: 301; }
.canvas-lbox{ position: absolute; width:243px; height:243px; top: 26px; left: 26px; }
.canvas-img{ width:243px; height:243px; z-index: 298; }
.canvas-list{ position: absolute; left: 0; top: 0; width: inherit; height: inherit; z-index: 299; }
.canvas-item{ position: absolute; left: 0; top: 0; width: 100%; height: 100%; color: #e21b58; text-shadow: 0 1px 1px rgba(255,255,255,.6); }
.canvas-item-text{ position: relative; display: block; padding-top: 20px; font-size: 36rpx; margin: 0 auto; text-align: center; -webkit-transform-origin: 50% 120px; transform-origin: 50% 120px; z-index: 300; }
.isMask { width: 100%; height: 100%; position: fixed; top: 0; left: 0; z-index: 1000; background: #000; opacity: 0.5; }
.isLogin { position: fixed; padding: 50rpx 50rpx; width: 480rpx; height: 335rpx; background: #fff; top: 50%; left: 50%; margin-left: -280rpx; margin-top: -185rpx; z-index: 1001; }
.isLoginBtn { display: flex; justify-content: space-around; }
.isLoginBtn button { background: #fff; width: 100%; height: 68repx; padding-left: 0rpx; padding-right: 0rpx; margin-right: 0rpx; margin-left: 0rpx; font-size: 28rpx; margin-top: 50rpx; color: #4bad37; }
.loginTitle { font-size: 34rpx; }
.loginshopImg { height: 80rpx; font-size: 26rpx; padding: 20rpx 0rpx; border-bottom: 1rpx solid #ececec; }
.loginshopImg>image { width: 68rpx; height: 68rpx; border-radius: 50%; margin-right: 20rpx; }
.isAgary { display: flex; align-items: center; font-size: 24rpx; color: #666; margin-top: 30rpx; }
.isAgary>text { margin-right: 20rpx; }
