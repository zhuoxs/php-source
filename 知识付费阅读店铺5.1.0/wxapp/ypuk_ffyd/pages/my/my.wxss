@import "../../resource/wxParse/wxParse.wxss";

.HelpExamineMy {
    background: #fff;
    padding: 20rpx;
}

.userInfo {
    text-align: center;
    position: absolute;
    z-index: 100;
    top: 60rpx;
    left: 0;
    right: 0;
    margin: auto;
}

.userInfo .userAvatar {
    width: 160rpx;
    height: 160rpx;
    border-radius: 50%;
    margin-top: 40rpx;
    margin-bottom: 10rpx;
}

.userInfo .userName {
    padding-bottom: 10rpx;
    color: #fff;
}

.userInfo .userVip {
    font-size: 12px;
    color: #fff;
    padding-bottom: 30rpx;
}

.MenuList {
    margin-top: 500rpx;
}

.MenuList .MenuItem {
    height: 70rpx;
    padding-left: 35rpx;
    padding-top: 30rpx;
    font-size: 28rpx;
    display: flex;
    flex-direction: row;
    border-bottom: 1rpx solid #eeeeee;
    background: #fff;
    position: relative;
}

.MenuList .MenuItem.ContactBtn {
    height: 100rpx;
    line-height: 40rpx;
    border-radius: 0;
}

.MenuList .MenuItem.ContactBtn::after {
    border: 0;
}

.MenuItem .MenuUrl {
    width: 100%;
}

.MenuItem .MenuUrl {
    width: 100%;
}

.MenuItem .MenuIcon {
    width: 50rpx;
    height: 50rpx;
    float: left;
}

.MenuItem .MenuText {
    padding-left: 25rpx;
    float: left;
    padding-top: 4rpx;
}

@keyframes move_wave {
    0% {
        transform: translateX(0) translateZ(0) scaleY(1);
    }

    50% {
        transform: translateX(-25%) translateZ(0) scaleY(0.55);
    }

    100% {
        transform: translateX(-50%) translateZ(0) scaleY(1);
    }
}

.waveWrapper {
    overflow: hidden;
    position: absolute;
    left: 0;
    right: 0;
    top: 0;
    height: 520rpx;
    margin: auto;
}

.waveWrapperInner {
    position: absolute;
    width: 100%;
    overflow: hidden;
    height: 100%;
    bottom: -2rpx;
    background-image: linear-gradient(to top,#ff7979 20%,#ee4f4f 80%);
}

.bgTop {
    z-index: 10;
    opacity: 0.75;
}

.bgBottom {
    z-index: 5;
}

.wave {
    position: absolute;
    left: 0;
    width: 200%;
    height: 100%;
    background-repeat: repeat no-repeat;
    background-position: 0 bottom;
    transform-origin: center bottom;
}

.waveTop {
    background-image: url('data:image/png;base64,');
    background-size: 50% 240rpx;
}

.waveAnimation .waveTop {
    animation: move_wave 12s linear infinite;
}

.waveBottom {
    background-image: url('data:image/png;base64,');
    background-size: 50% 200rpx;
}

.waveAnimation .waveBottom {
    animation: move_wave 15s linear infinite;
}